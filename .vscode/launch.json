  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Module",
      "type": "python",
      "request": "launch",
      "module": "uvicorn",
      "args": ["app.main:app", "--reload"],
      "justMyCode": true
    },
    {
      "name": "Python: Current File",
      "type": "python",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal"
    },
    {
      "name": "Python: UNVICORN",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/uvicorn app.main:app --reload",
      "console": "integratedTerminal"
    }
  ]
}
